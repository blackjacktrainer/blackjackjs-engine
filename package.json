{
  "name": "@blackjacktrainer/blackjackjs-engine",
  "version": "0.4.0",
  "description": "Core game logic for a Blackjack basic strategy trainer",
  "main": "dist/main.js",
  "scripts": {
    "browser": "webpack-dev-server --open",
    "start": "APP_ROOT=$(npm prefix) node dist/main.node.js",
    "test": "NODE_PATH=./src mocha --require @babel/register",
    "lint": "eslint --fix .",
    "build": "webpack"
